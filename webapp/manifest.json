{
	"_version": "1.48.0",
	"sap.app": {
		"id": "tronox",
		"type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "neomii"
        }
	},
    "sap.ui": {
        "technology": "UI5",
        "icons": {
          "icon": "",
          "favIcon": "",
          "phone": "",
          "phone@2": "",
          "tablet": "",
          "tablet@2": ""
        },
        "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
        },
        "supportedThemes": [
            "sap_belize"
        ]
      },
	"sap.ui5": {
		"flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.111.0",
            "libs": {
                "sap.m": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "resources": {
			"css": [
				{
					"uri": ""
				}
			]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tronox.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": ""
			}
		},
        "rootView": {
			"viewName": "tronox.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		}
	}
}
