{
	"_version": "1.48.0",
    "start_url": "index.html",
	"sap.app": {
		"id": "tronox",
		"type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json"

	},
    "sap.ui": {
        "technology": "UI5",
        "icons": {
          "icon": "",
          "favIcon": "",
          "phone": "",
          "phone@2": "",
          "tablet": "",
          "tablet@2": ""
        },
        "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
        },
        "supportedThemes": [
          "sap_belize_plus"
        ]
      },
	"sap.ui5": {
		"flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.117.0",
            "libs": {
                "sap.m": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			],
            "js": [
                {
                    "uri": "js/Transactions.js"
                }
            ]
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tronox.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": ""
			}
		},
        "routing": {
           "config": {
              "routerClass": "sap.m.routing.Router",
              "type": "View",
              "viewType": "XML",
              "path": "tronox.view",
              "controlId": "app",
              "controlAggregation": "pages",
              "transition": "slide",
              "bypassed": {
                "target": "notFound"
              },
              "async": true
           },
           "routes": [
                {
                    "pattern": "",
                    "name": "appHome",
                    "target": "home"
                },
                {
                    "pattern": "workcenters",
				    "name": "workcenterPanel",
				    "target": "workcenters"
                },
                {
                    "pattern": "inspection",
				    "name": "inspectionPanel",
				    "target": "inspection"
                },
                {
                    "pattern": "material",
				    "name": "materialPanel",
				    "target": "material"
                },
                {
                    "pattern": "characteristic",
				    "name": "characteristicPanel",
				    "target": "characteristic"
                },
                {
                    "pattern": "rule",
				    "name": "rulePanel",
				    "target": "rule"
                },
                {
                    "pattern": "actionCatalog",
				    "name": "actionCatalogPanel",
				    "target": "actionCatalog"
                },
                {
                    "pattern": "usage",
				    "name": "usagePanel",
				    "target": "usage"
                },
                {
                    "pattern": "statistic",
				    "name": "statisticPanel",
				    "target": "statistic"
                },
                {
                    "pattern": "alarm",
				    "name": "alarmPanel",
				    "target": "alarm"
                },
                {
                    "pattern": "dashboard",
				    "name": "dashboardPanel",
				    "target": "dashboard"
                },
                {
                    "pattern": "lotBatch",
				    "name": "lotBatchPanel",
				    "target": "lotBatch"
                },
                {
                    "pattern": "resultRecording",
				    "name": "resultRecordingPanel",
				    "target": "resultRecording"
                },
                {
                    "pattern": "bulkRecording",
				    "name": "bulkRecordingPanel",
				    "target": "bulkRecording"
                },
                {
                    "pattern": "sampleDashboard",
				    "name": "sampleDashboardPanel",
				    "target": "sampleDashboard"
                }
           ],
           "targets": {
              "home": {
                 "id": "home",
                 "name": "Home",
                 "level" : 1
              },
              "notFound": {
                 "id": "notFound",
                 "name": "NotFound",
                 "transition": "show"
              },
              "workcenters": {
                 "id": "workcenterPanel",
                 "path": "tronox.view.configurations.workcenters",
                 "name": "WorkcenterPanel",
                 "level" : 2
              },
              "inspection": {
                 "id": "inspectionPanel",
                 "path": "tronox.view.configurations.inspections",
                 "name": "InspectionPanel",
                 "level" : 2
              },
              "material": {
                 "id": "materialPanel",
                 "path": "tronox.view.configurations.materials",
                 "name": "MaterialPanel",
                 "level" : 2
              },
              "characteristic": {
                 "id": "characteristicPanel",
                 "path": "tronox.view.configurations.characteristics",
                 "name": "CharacteristicPanel",
                 "level" : 2
              },
              "rule": {
                 "id": "rulePanel",
                 "path": "tronox.view.configurations.rule",
                 "name": "RulePanel",
                 "level" : 2
              },
              "actionCatalog": {
                 "id": "actionCatalogPanel",
                 "path": "tronox.view.configurations.actionCatalog",
                 "name": "ActionCatalogPanel",
                 "level" : 2
              },
              "usage": {
                 "id": "usagePanel",
                 "path": "tronox.view.quality.usage",
                 "name": "UsagePanel",
                 "level" : 2
              },
              "statistic": {
                 "id": "statisticPanel",
                 "path": "tronox.view.quality.statistics",
                 "name": "StatisticPanel",
                 "level" : 2
              },
              "alarm": {
                 "id": "alarmPanel",
                 "path": "tronox.view.quality.alarms",
                 "name": "AlarmPanel",
                 "level" : 2
              },
              "dashboard": {
                 "id": "dashboardPanel",
                 "path": "tronox.view.quality.dashboard",
                 "name": "dashboardPanel",
                 "level" : 2
              },
              "lotBatch": {
                 "id": "lotBatchPanel",
                 "path": "tronox.view.quality.lotBatch",
                 "name": "LotBatchPanel",
                 "level" : 2
              },
              "resultRecording": {
                 "id": "resultRecordingPanel",
                 "path": "tronox.view.quality.resultRecording",
                 "name": "ResultRecordingPanel",
                 "level" : 2
              },
              "bulkRecording": {
                 "id": "bulkRecordingPanel",
                 "path": "tronox.view.quality.bulkRecording",
                 "name": "BulkRecordingPanel",
                 "level" : 2
              },
              "sampleDashboard": {
                 "id": "sampleDashboardPanel",
                 "path": "tronox.view.quality.sampleDashboard",
                 "name": "SampleDashboardPanel",
                 "level" : 2
              }
           }
        },
        "rootView": {
			"viewName": "tronox.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		}
	}
}
